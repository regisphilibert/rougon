{{- .Scratch.SetInMap "item" "title" .Title -}}
{{- .Scratch.SetInMap "item" "created" .Date -}}
{{- with .Params.lastMod -}}
{{- $.Scratch.SetInMap "item" "updated" . -}}
{{- end -}}
{{- with .Params.tags -}}
{{- $.Scratch.SetInMap "item" "tags" . -}}
{{- end -}}
{{- with .Params.categories -}}
{{- $.Scratch.SetInMap "item" "categories" . -}}
{{- end -}}
{{- .Scratch.SetInMap "item" "permalink" (partial "formatUrl.json" (dict "context" . "url" .RelPermalink)) -}}
{{- .Scratch.SetInMap "item" "type" .Type -}}